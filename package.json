{
  "name": "fullstack-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "author": "villett",
  "license": "ISC",
  "main": "server/index.js",
  "scripts": {
    "dev:frontend": "cd client && vite",
    "dev:backend": "cd server && nodemon index.js",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "test": "cd server && vitest",
    "coverage": "cd server && vitest run --coverage",
    "prepare": "husky && husky install",
    "format": "lint-staged",
    "coverage-check": "cd server && vitest run --coverage && node scripts/check-coverage.cjs"
  },
  "lint-staged": {
    "server/**/*.{js,ts,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "client/**/*.{js,ts,json,css,md,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
